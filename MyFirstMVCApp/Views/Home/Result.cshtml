@model List<MyFirstMVCApp.Models.Person>

<head>
    <link href="~/StyleSheet.css" rel="stylesheet" />
</head>
<table>
    <thead>
        <tr>
            <th>Year</th>
            <th>Honor</th>
            <th>Name</th>
            <th>Country</th>
            <th>Birth Year</th>
            <th>Death Year</th>
            <th>Title</th>
            <th>Category</th>
            <th>Context</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Year</td>
                <td>@item.Honor</td>
                <td>@item.Name</td>
                <td>@item.Country</td>
                <td>@item.Birth_Year</td>
                <td>@item.DeathYear</td>
                <td>@item.Title</td>
                <td>@item.Category</td>
                <td>@item.Context</td>
            </tr>
        }
    </tbody>
</table>